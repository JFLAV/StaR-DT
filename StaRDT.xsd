	<xs:element name="PerimetreParticulier" type="star-dt:PerimetreParticulierType" substitutionGroup="star-dt:Etiquette">
		<xs:annotation>
			<xs:documentation>-- Name --
-- Definition --
Classe qui définit des périmètres présentant une particularité. 
-- Description --
La description de la zone peut être communiquée dans la description d’un objet Etiquette associé.
-- Source --</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="PerimetreParticulierType">
		<xs:complexContent>
			<xs:extension base="star-dt:EtiquetteType">
				<xs:sequence>
					<xs:element name="geometrie" type="gml:SurfacePropertyType">
